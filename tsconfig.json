// prettier-ignore
{
  // "extends": "@tsconfig/react-native/tsconfig.json",     /* Recommended React Native TSConfig base */
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution":"node",
    "lib": ["ESNext", "DOM"],
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true, // important to ignore types from node_modules
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ]
}
